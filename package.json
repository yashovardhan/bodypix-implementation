{
  "name": "bodypix-implementation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "@tensorflow-models/body-pix": "1.0.0",
    "@tensorflow/tfjs": "1.0.0",
    "core-js": "2"
  },
  "devDependencies": {
    "babel-core": "~6.26.3",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~1.6.1",
    "clang-format": "~1.2.2",
    "cross-env": "^5.2.0",
    "eslint": "~4.19.1",
    "eslint-config-google": "~0.9.1",
    "parcel-bundler": "~1.10.3",
    "yalc": "~1.0.0-pre.23"
  },
  "scripts": {
    "watch": "cross-env NODE_ENV=development parcel index.html --no-hmr --open ",
    "build": "cross-env NODE_ENV=production parcel build index.html  --no-minify --public-url ./",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": "google",
    "rules": {
      "require-jsdoc": 0,
      "valid-jsdoc": 0
    },
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    }
  },
  "eslintIgnore": [
    "dist/"
  ],
