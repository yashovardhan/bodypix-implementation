{
  "name": "bodypix-implementation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "@tensorflow-models/body-pix": "^1.1.1",
    "@tensorflow/tfjs": "^1.2.3"
